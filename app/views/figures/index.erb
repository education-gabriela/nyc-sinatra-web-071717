<h1>All Figures</h1>
<ul>
  <% @figures.each do |figure| %>
      <li>
        <strong><%= figure.name %></strong> |
        <a href="/figures/<%= figure.id %>" title="Show">Show</a> |
        <a href="/figures/<%= figure.id %>/edit" title="Edit">Edit</a>
      </li>
  <% end %>
</ul>
<a href="/figures/new">New Figure</a>