<h1>All Landmarks</h1>
<ul>
  <% @landmarks.each do |landmark| %>
      <li>
        <strong><%= landmark.name %></strong> - <%= landmark.year_completed %> |
        <a href="/landmarks/<%= landmark.id %>" title="Show">Show</a> |
        <a href="/landmarks/<%= landmark.id %>/edit" title="Edit">Edit</a>
      </li>
  <% end %>
</ul>
<a href="/landmarks/new">New Landmark</a>